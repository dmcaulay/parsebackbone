<h1>Edit game_score</h1>

<form id="edit-game_score" name="game_score">
  <div class="field">
    <label for="team"> team:</label>
    <input type="text" name="team" id="team" value=<%= team %> >
  </div>

  <div class="field">
    <label for="score"> score:</label>
    <input type="text" name="score" id="score" value=<%= score %> >
  </div>

  <div class="actions">
    <input type="submit" value="Update Game score" />
  </div>

</form>

